{
  "name": "bvtnet-items-provider",
  "version": "0.5.1",
  "description": "datatables.net ajax items provider for bootstrap-vue b-table",
  "author": "Tom Noogen",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [
    "bootstrap-vue",
    "bootstrap",
    "javascript",
    "pure",
    "css",
    "simple",
    "datatables.net",
    "jquery",
    "provider",
    "b-table",
    "vue"
  ],
  "repository": "git://github.com/niiknow/bvtablenet-items-provider.git",
  "scripts": {
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "build": "npm run production",
    "production": "cross-env NODE_ENV=build node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "cross-env NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/ava",
    "watch-test": "npm run test -- --watch",
    "lint": "eslint --ext .js,.vue src/",
    "lint-fix": "eslint --fix --ext .js,.vue src/",
    "check-outdated": "npm outdated"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "ava": "^2.3.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "bootstrap-vue": "^2.1.0",
    "browser-env": "^3.3.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.1",
    "es6-promise": "^4.2.8",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^3.0.1",
    "laravel-mix": "^4.1.2",
    "nyc": "^14.1.1",
    "raw-loader": "^3.1.0",
    "smart-banner-webpack-plugin": "^3.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "ava": {
    "require": [
      "./tests/_setup-browser-env.js"
    ]
  }
}
